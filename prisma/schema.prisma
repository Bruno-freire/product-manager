generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  store     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model List {
  id        Int      @id @default(autoincrement())
  store     String
  createdAt DateTime @default(now())
  products  Json
}
